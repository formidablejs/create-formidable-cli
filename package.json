{
  "name": "create-formidable-cli",
  "version": "0.0.2",
  "description": "Create a Formidable CLI",
  "author": "Donald Pakkies @donaldp",
  "bin": {
    "create-formidable-cli": "./bin/run",
    "cfc": "./bin/run"
  },
  "homepage": "https://github.com/formidablejs/create-formidable-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "formidablejs/create-formidable-cli",
  "files": [
    "/assets",
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@formidablejs/installer": "^0.9.5",
    "@inquirer/prompts": "^2.1.1",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.22",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.35",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "create-formidable-cli",
    "dirname": "create-formidable-cli",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/formidablejs/create-formidable-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
